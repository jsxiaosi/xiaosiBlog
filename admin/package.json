    "http-server": "^14.1.1",
    "js-cookie": "^3.0.1",
    "lint-staged": "^13.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.5.1",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.0.0",
