    "vue-types": "^4.2.1",
    "vuex": "^4.0.2",
    "wangeditor": "^4.7.15"
  },
  "devDependencies": {
    "@ehutch79/vite-eslint": "^0.0.1",
    "@types/core-js": "^2.5.5",
