    "http-server": "^14.1.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^12.3.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
